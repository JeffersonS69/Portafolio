<script>
  function detectCurrentSection() {
    const sections: NodeListOf<HTMLElement> =
      document.querySelectorAll("article");
    let currentSection: string = "";

    sections.forEach((section: HTMLElement) => {
      const sectionTop: number = section.offsetTop;
      const sectionHeight: number = section.offsetHeight;
      const scrollPosition: number = window.scrollY;

      if (scrollPosition >= sectionTop - sectionHeight / 2) {
        currentSection = section.getAttribute("id") as string;
      }
    });

    if (currentSection && window.location.hash !== `#${currentSection}`) {
      history.pushState(null, "", `#${currentSection}`);
    }
  }

  window.addEventListener("scroll", detectCurrentSection);

  const paragraph1 = document.getElementById(
    "paragraph1",
  ) as HTMLParagraphElement;
  const paragraph2 = document.getElementById(
    "paragraph2",
  ) as HTMLParagraphElement;
  const toggleButton1 = document.getElementById(
    "toggleButton1",
  ) as HTMLButtonElement;

  const toggleButton2 = document.getElementById(
    "toggleButton2",
  ) as HTMLButtonElement;

  toggleButton1.addEventListener("click", () => {
    paragraph1.classList.remove("active-para1");
    paragraph1.classList.add("hidden-para2");
    paragraph2.classList.remove("hidden-para2");
    paragraph2.classList.add("active-para1");
  });

  toggleButton2.addEventListener("click", () => {
    paragraph2.classList.remove("active-para1");
    paragraph2.classList.add("hidden-para2");
    paragraph1.classList.remove("hidden-para2");
    paragraph1.classList.add("active-para1");
  });

  document.getElementById("downloadCV")?.addEventListener("click", () => {
    const link = document.createElement("a");

    link.href =
